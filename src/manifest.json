{
	"_version": "1.12.0",

	"sap.app": {
		"id": "ui5-ts-shim-showcase",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{app_title}}",
		"description": "{{app_description}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {

		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "ui5-ts-shim-showcase.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.94.0",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ui5-ts-shim-showcase.i18n.i18n"
				}
			}
		},

		"routing": {
			
		}
	}
}